@model IEnumerable<CRUDProcess.Models.Student>
@{
    ViewData["Title"] = "Home Page";
}
<a class="btn btn-primary" asp-action="Student" asp-route-id="null">Ekle</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Age</td>
            </tr>
        }
    </tbody>
</table>